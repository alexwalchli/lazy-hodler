{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es7"],
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "outDir": "../../_build-bot",
    "declaration": true
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}

