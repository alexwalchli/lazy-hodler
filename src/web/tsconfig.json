{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es7", "dom"],
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "noEmit": true,
    "declaration": true,
    "jsx": "react"
  },
  "include": [
    "./*"
  ],
  "exclude": [
    "node_modules"
  ]
}

